# Aadee Chat AI Business Operations Copilot - Environment Variables

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google APIs Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret_here
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8000/api/oauth/callback

# Google Calendar Configuration
CALENDAR_ID=primary
SLOT_MINUTES=30

# Business Platform APIs (for future integration)
YELP_API_KEY=your_yelp_api_key_here
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here

# Frontend Configuration (for website/.env)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_API_BASE=http://localhost:8000
VITE_PUBLIC_BACKEND_URL=http://localhost:8000

# Modal Configuration
MODAL_TOKEN_ID=your_modal_token_id_here
MODAL_TOKEN_SECRET=your_modal_token_secret_here

# Development Configuration
DEBUG=true
LOG_LEVEL=info
ENVIRONMENT=development

# Security Configuration
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Database Configuration (if using local database for development)
DATABASE_URL=postgresql://user:password@localhost:5432/aadee_dev

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_here

# Analytics Configuration
GOOGLE_ANALYTICS_ID=your_ga_id_here

# Monitoring Configuration
SENTRY_DSN=your_sentry_dsn_here

# File Upload Configuration
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,pdf,doc,docx

# Cache Configuration
REDIS_URL=redis://localhost:6379

# Feature Flags
ENABLE_SOCIAL_MEDIA_INTEGRATION=false
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_WEBHOOK_NOTIFICATIONS=false
